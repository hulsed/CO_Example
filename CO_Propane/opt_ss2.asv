function J2=opt_ss2(z)

K8=0.1;
K10=0.1;
P=40;
initpt=[1,1]

[x,J1,exitflag,output]=fmincon(@ss1_obj,initpt,[],[],[],[],[],[],@ss1_con, options)

    function obj=ss1_obj(x1)
        x8=x(1);
        x10=x(2);
        
        obj=(x8-z(8))^2+(x10-z(10))^2;
        
    end

    function [ceq,cineq]=ss1_con(x1)
        x8=x(1);
        x10=x(2);
        
        f8=K8*z(1)-z(4)*x8*P/z(11);
        f10=K10*z(11)
        
        ceq=[f8,f10];
        cineq=[];
        
    end

end